{
    "name": "@nody-org/logger",
    "version": "1.0.2",
    "license": "MIT",
    "private": false,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "jest": {
        "preset": "@nody-org/jest/node"
    },
    "exports": {
        ".": {
            "node": "./dist/cjs/index.js",
            "default": "./dist/cjs/index.js"
        },
        "./package.json": "./package.json"
    },
    "typesVersions": {
        ">=4.0": {
            "*": [
                "dist/types/*"
            ]
        }
    },
    "scripts": {
        "lint": "eslint src/",
        "lint:fix": "eslint --fix src/",
        "test": "yarn cross-env NODE_ENV=test jest --passWithNoTests --collect-coverage",
        "test:unit": "yarn cross-env NODE_ENV=test jest --passWithNoTests --collect-coverage __tests__/unit",
        "test:integration": "yarn cross-env NODE_ENV=test jest --passWithNoTests --collect-coverage __tests__/integration",
        "build:package": "tsc",
        "build:clean": "rimraf dist",
        "build:code": "esno ./scripts/bundle.ts",
        "build:types": "tsc --emitDeclarationOnly --outDir dist/types",
        "build": "run-s build:clean build:code build:types",
        "clean": "rimraf node_modules dist coverage .eslintcache",
        "minor": "yarn version --minor",
        "major": "yarn version --major",
        "patch": "yarn version --patch",
        "publish:version": "yarn publish . --tag $npm_package_version --non-interactive",
        "publish:latest": "yarn publish . --tag latest --non-interactive",
        "preversion": "yarn test",
        "postversion": "git push --tags && yarn build && yarn publish:latest && git push && echo \"Successfully released version $npm_package_version!\""
    },
    "dependencies": {
        "winston": "^3.4.0"
    },
    "devDependencies": {
        "@types/winston": "^2.4.4",
        "aws-sdk": "^2.1062.0",
        "esbuild": "^0.14.25",
        "esno": "^0.14.1",
        "npm-run-all": "^4.1.5",
        "winston-cloudwatch": "^3.1.1"
    },
    "optionalDependencies": {
        "aws-sdk": "^2.1062.0",
        "winston-cloudwatch": "^3.1.1"
    }
}
