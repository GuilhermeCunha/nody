{
    "name": "@nody-org/logger",
    "version": "1.0.10",
    "license": "MIT",
    "private": false,
    "main": "./index.js",
    "types": "./index.d.ts",
    "jest": {
        "preset": "@nody-org/jest/node"
    },
    "scripts": {
        "lint": "eslint src/",
        "lint:fix": "eslint --fix src/",
        "test": "yarn cross-env NODE_ENV=test jest --passWithNoTests --collect-coverage",
        "test:unit": "yarn cross-env NODE_ENV=test jest --passWithNoTests --collect-coverage __tests__/unit",
        "test:integration": "yarn cross-env NODE_ENV=test jest --passWithNoTests --collect-coverage __tests__/integration",
        "build:package": "esno ./scripts/package.ts",
        "build:clean": "rimraf dist tsconfig.tsbuildinfo",
        "build:code": "tsc",
        "build": "run-s build:clean build:code build:package",
        "clean": "rimraf node_modules dist coverage .eslintcache",
        "minor": "yarn version --minor",
        "major": "yarn version --major",
        "patch": "yarn version --patch",
        "publish:version": "yarn publish dist --tag $npm_package_version --non-interactive",
        "publish:latest": "yarn publish dist --tag latest --non-interactive",
        "preversion": "yarn test && yarn build",
        "postversion": "git push --tags && yarn build && yarn publish:latest && git push && echo \"Successfully released version $npm_package_version!\""
    },
    "dependencies": {
        "tslib": "^2.4.0",
        "winston": "^3.4.0"
    },
    "devDependencies": {
        "@aws-sdk/client-cloudwatch-logs": "^3.0.0",
        "@types/winston": "^2.4.4",
        "esno": "^0.14.1",
        "npm-run-all": "^4.1.5",
        "winston-cloudwatch": "^6.0.1"
    },
    "optionalDependencies": {},
    "peerDependencies": {
        "@aws-sdk/client-cloudwatch-logs": "^3.0.0",
        "winston-cloudwatch": "^6.0.1"
    }
}
